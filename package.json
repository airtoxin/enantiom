{
  "name": "enantiom",
  "version": "3.0.0-6",
  "bin": {
    "enantiom": "src/index.js"
  },
  "scripts": {
    "predev:run": "run-s clean",
    "dev:run": "ts-node src/index.ts run --config example/enantiom.config.json -vvv",
    "predev:gen": "run-s clean",
    "dev:gen": "ts-node src/index.ts generate-region -vvv",
    "preview": "serve dist",
    "prebuild": "run-s clean",
    "build": "tsc",
    "format": "prettier --ignore-path .gitignore --write .",
    "pretest": "run-s clean",
    "test": "run-s site:lint test:unit",
    "test:unit": "jest",
    "site:dev": "next dev",
    "site:build": "next build && next export",
    "site:lint": "next lint",
    "clean": "rimraf 'src/**/*.{js,jsx}'",
    "prepublish": "run-s clean build"
  },
  "repository": "git@github.com:airtoxin/enantiom.git",
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@aws-sdk/client-s3": "^3.30.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.7.4",
    "@types/object-hash": "2.1.1",
    "@types/react": "^17.0.19",
    "@types/recursive-readdir": "^2.2.0",
    "@types/sharp": "^0.29.2",
    "@types/temp": "^0.9.1",
    "@types/url-join": "^4.0.1",
    "antd": "^4.16.13",
    "commander": "^8.2.0",
    "date-fns": "^2.23.0",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.32",
    "next": "^11.1.1",
    "object-hash": "^2.2.0",
    "odiff-bin": "^2.4.2",
    "p-any": "^3.0.0",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.1",
    "playwright": "^1.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-image-lightbox": "^5.1.4",
    "react-json-tree": "^0.15.0",
    "recursive-readdir": "^2.2.2",
    "sharp": "^0.29.1",
    "temp": "^0.9.4",
    "typescript": "^4.4.2",
    "url-join": "^4.0.1",
    "utility-types": "^3.10.0",
    "zod": "^3.8.1"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  },
  "files": [
    "src",
    "tsconfig.json",
    "next.config.js",
    "next-env.d.ts",
    "public/images",
    "yarn.lock"
  ]
}
